<template>
  <div>
    <div class="line">
      <span class="comment">/</span>
      <span class="comment comment_active">{{head}}</span> 
    </div>

    <div class="line">
      <span class="white-space space"> </span>
      <span class="comment comment_active" v-for="item in menuNameMain">{{ item }}</span>
    </div>
    <div class="line">
      <span class="comment comment_active">{{head}}</span> 
      <span class="comment">/</span>
    </div>
  </div>
</template>

<script>
 import './style.sass'
 export default {
   name: 'menu',
   props: ['menuNameMain'],
   computed: {
     head: function() {
       return '*'.repeat(8);
     }
   }
 }
</script>

